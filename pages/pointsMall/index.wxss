.all{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  overflow: scroll;
  background-color: #fafafa;
}
.line{
  width: 6rpx;
  height: 50rpx;
  background-color: orange;
  margin: 35rpx 10rpx 35rpx 15rpx;
}
.title{
  height: 50rpx;
  margin: 35rpx 0rpx 0rpx 10rpx;
}
.allCoupons{
  background-color: #fff;
  margin: 20rpx;
  border: 1rpx solid #f8f8f8;
  box-shadow: 4rpx 4rpx 3rpx #f8f8f8;
}
.coupons{
  display: grid;
  grid-template-columns: 200rpx auto 30rpx;
  height: 220rpx;
  border-bottom: 1rpx dashed #f8f8f8;
}
.left{
  text-align: center;

}
.price{
  color: #E64340;
  font-size: 58rpx;
  font-weight: bold;
  margin: 30rpx 0;
}
.hreshold{
  font-size: 28rpx;
  color: #666;
}
.right{
  text-align: right;
}
.cutPrice{
  margin-top: 20rpx;
  letter-spacing: 4rpx;
  font-size: 34rpx;
}
.endDate{
  margin: 8rpx 0rpx;
  font-size: 28rpx;
  color: #888;
}
.couponsType{
  display: inline-block;
  font-size: 25rpx;
  color: #888;
  padding: 8rpx;
  border-radius: 8rpx;
  border: 1rpx solid #888;
}
.points{
  width: 360rpx;
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
  margin: 15rpx;
  background-color: #f8f8f8;
  border-radius: 40rpx;
}
.needPoint{
  width: 160rpx;
  text-align: center;
  color: #E64340;
}
.exchangeBtn{
  width: 200rpx;
  border-radius: 40rpx;
  text-align: center;
  color: #fff;
  background-color: #E64340;
}